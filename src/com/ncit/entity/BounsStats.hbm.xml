<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.ncit.entity">

    <class name="BounsStats" table="ncit_bounsstats" lazy="false">
    
        <id name="id">
            <generator class="native"/>
        </id>
        
        <property name="financeId"/>
        <property name="department"/>
        <property name="projectName"/>
        
        <property name="bounsTotal"/>
        <property name="bounsSendTotal"/>
        <property name="bounsUseTotal"/>
        <property name="bounsUsedTotal"/>
        <property name="balance"/>
        
    </class>

</hibernate-mapping>
