<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.ncit.entity">

    <class name="BounsUse" table="ncit_bounsuse" lazy="false">
    
        <id name="id">
            <generator class="native"/>
        </id>
        
        <property name="financeId"/>
        <!-- <property name="department"/> -->
        <property name="projectName"/>
        
        <property name="fundUseUp"/>
        <property name="fundRecommend"/>
        <property name="expenseReason"/>
        <property name="expenseMoney"/>
        <property name="expenseDate"/>
        <property name="alert"/>
        
    </class>

</hibernate-mapping>
